    "make-coverage-badge": "node scripts/make-coverage-badge.js",
    "docs": "typedoc --mode file --exclude **/sandbox.ts --out docs ./src"
  },
  "author": "Dylan Bienenstock",
  "license": "ISC",
  "devDependencies": {
    "alsatian": "^3.0.0-alpha.2",
    "coveralls": "^3.0.3",
    "make-coverage-badge": "^1.1.1",
    "nyc": "^13.3.0",
    "tap-bark": "^1.0.0",
    "ts-node": "^8.0.3",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.4000"
  },
  "dependencies": {}
}
